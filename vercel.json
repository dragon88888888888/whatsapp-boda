{
  "version": 2,
  "builds": [
    {
      "src": "chatwhats.js",
      "use": "@vercel/node"
    }
  ],
  "routes": [
    {
      "src": "/(.*)",
      "dest": "chatwhats.js"
    }
  ],
  "env": {
    "NODE_ENV": "production"
  },
  "functions": {
    "chatwhats.js": {
      "maxDuration": 60
    }
  }
}
